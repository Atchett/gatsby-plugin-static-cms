{
  "name": "gatsby-plugin-static-cms",
  "description": "A Gatsby plugin which generates the Static CMS single page app",
  "version": "4.0.0",
  "license": "MIT",
  "repository": "https://github.com/StaticJsCMS/gatsby-plugin-static-cms.git",
  "bugs": "https://github.com/StaticJsCMS/gatsby-plugin-static-cms/issues",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "netlify",
    "static-cms",
    "cms"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "copy-webpack-plugin": "^13.0.0",
    "html-webpack-plugin": "^5.6.3",
    "html-webpack-tags-plugin": "^3.0.2",
    "lodash": "^4.17.21",
    "netlify-identity-widget": "^1.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@staticcms/app": "^4.3.0",
    "babel-preset-gatsby-package": "^3.13.1",
    "cross-env": "^7.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "homepage": "https://github.com/StaticJsCMS/gatsby-plugin-static-cms",
  "main": "index.js",
  "peerDependencies": {
    "@staticcms/app": "^4.1.4",
    "gatsby": "^5.14.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "webpack": "^5.0.0"
  }
}
